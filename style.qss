}

QWidget#MainDataWidget-totalWidget>QLabel {
    max-height: 20px;
    border-radius: 10px;
}

#MainDataWidget-totalWidget {
    min-width: 300px;
}

#MainDataWidget-tableWidget {
    min-width: 700px;
}
/* End Widget1 */
